@{
}

@model Intro_MVC.Models.DoggyDayCare.FormResultViewModel

<ul class="list-group">
    @foreach (var dog in Model.Dogs)
    {
        <li class="list-group-item">
            <p>Name: @dog.Name</p>
            <p>Day of the week: @dog.DayOfTheWeek</p>
            <a asp-controller="DoggyDayCare" asp-action="UpdateDog" asp-route-id="@dog.ID">Update Dog</a>
            <br />
            <a class="text-danger" asp-controller="DoggyDayCare" asp-action="DeleteDog" asp-route-id="@dog.ID">Delete Dog</a>
        </li>
    }
</ul>
